#### UI ####

[org/gnome/desktop/background]
picture-uri='file://{{ ansible_env.HOME }}/.local/share/backgrounds/warty-final-ubuntu-16.04.png'

[org/gnome/desktop/interface]
gtk-key-theme='Emacs'
clock-show-weekday=true
clock-show-seconds=true
cursor-size=32
text-scaling-factor=1.4

[org/gnome/desktop/notifications]
show-in-lock-screen=false

[org/gnome/desktop/screensaver]
picture-uri='file://{{ ansible_env.HOME }}/.local/share/backgrounds/warty-final-ubuntu-16.04.png'

[org/gnome/desktop/wm/preferences]
button-layout='close,minimize,maximize:'

[org/gnome/mutter]
dynamic-workspaces=false

[org/gnome/shell]
enabled-extensions=['clock-override@gnomeshell.kryogenix.org', 'impatience@gfxmonk.net', 'remove-dropdown-arrows@mpdeimos.com', 'system-monitor@paradoxxx.zero.gmail.com', 'unite@hardpixel.eu']
favorite-apps=['org.gnome.Nautilus.desktop', 'firefox.desktop', 'chromium_chromium.desktop', 'code_code.desktop', 'gnome-control-center.desktop']

[org/gnome/shell/extensions/dash-to-dock]
background-color='#5e2750'
custom-background-color=true
dash-max-icon-size=32
transparency-mode='FIXED'
background-opacity=0.9
show-apps-at-top=true

[org/gnome/shell/extensions/desktop-icons]
show-home=false
show-trash=false

[org/gnome/shell/extensions/net/gfxmonk/impatience]
speed-factor=0.75

[org/gnome/shell/extensions/system-monitor]
background='#00000000'
icon-display=false
show-tooltip=true
center-display=true
move-clock=true
cpu-refresh-time=5000
cpu-show-text=false
memory-show-text=false
net-refresh-time=5000
net-show-text=false
net-speed-in-bits=true
disk-refresh-time=5000
freq-refresh-time=5000
cpu-iowait-color='#666666ff'
cpu-nice-color='#aea79fff'
cpu-system-color='#d6d3cfff'
cpu-user-color='#f6f6f5ff'
memory-buffer-color='#d6d3cfff'
memory-cache-color='#aea79fff'
memory-program-color='#f6f6f5ff'
net-collisions-color='#d6d3cfff'
net-down-color='#f6f6f5ff'
net-downerrors-color='#d6d3cfff'
net-up-color='#aea79fff'
net-uperrors-color='#d6d3cfff'

[org/gnome/shell/extensions/unite]
desktop-name-text=''
window-buttons-theme='yaru'

[org/gnome/desktop/search-providers]
disabled=['org.gnome.Software.desktop', 'org.gnome.Terminal.desktop', 'org.gnome.seahorse.Application.desktop', 'org.gnome.Nautilus.desktop', 'org.gnome.Calendar.desktop']


#### location service ####
[org/gnome/system/location]
enabled=true

[org/gnome/desktop/datetime]
automatic-timezone=true


#### touchpad and mouse ####

[org/gnome/desktop/peripherals/touchpad]
speed=1.0

[org/gnome/desktop/peripherals/mouse]
speed=1.0


#### power #####

[org/gnome/settings-daemon/plugins/power]
power-button-action='suspend'
critical-battery-action='shutdown'
sleep-inactive-ac-type='nothing'
sleep-inactive-battery-timeout=3600

[org/gnome/desktop/screensaver]
lock-delay=uint32 30

[org/gnome/desktop/session]
idle-delay=uint32 600

[org/gnome/power-manager]
info-history-graph-smooth=false


#### keyboard ####

[org/gnome/desktop/input-sources]
sources=[('ibus', 'mozc-jp'), ('xkb', 'jp'), ('xkb', 'us')]
xkb-options=['ctrl:nocaps']

[desktop/ibus/general]
preload-engine-mode=0
preload-engines=['mozc-jp', 'xkb:jp::jpn']

[com/canonical/indicator/keyboard]
migrated=true
visible=false


#### gnome-terminal ####

[org/gnome/terminal/legacy]
default-show-menubar=false
headerbar=@mb false

[org/gnome/terminal/legacy/profiles:]
list=['b1dcc9dd-5262-4d8d-a863-c897e6d979b9']
default='b1dcc9dd-5262-4d8d-a863-c897e6d979b9'

[org/gnome/terminal/legacy/profiles:/:b1dcc9dd-5262-4d8d-a863-c897e6d979b9]
audible-bell=false
background-transparency-percent=10
use-theme-transparency=false
use-transparent-background=true
visible-name='default'


#### diodon ####

[net/launchpad/diodon/keybindings]
history-accelerator=''

[net/launchpad/diodon/plugins]
active-plugins=@as []

[net/launchpad/diodon/clipboard]
recent-items-size=uint32 10
instant-paste=false
use-primary=false

[org/gnome/settings-daemon/plugins/media-keys]
custom-keybindings=['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/']

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0]
binding='<Primary><Alt>c'
command='diodon'
name='Diodon'


#### gedit #####

[org/gnome/gedit/preferences/encodings]
candidate-encodings=['UTF-8', 'CURRENT', 'CP932', 'ISO-8859-15', 'UTF-16']


#### deja-dup ####

[org/gnome/deja-dup]
periodic-period=7
backend='remote'
periodic=true
delete-after=182
exclude-list=['{{ ansible_env.HOME }}/.local/share/Trash', '{{ ansible_env.HOME }}/backup', '{{ ansible_env.HOME }}/Canonical', '{{ ansible_env.HOME }}/dev', '{{ ansible_env.HOME }}/Documents/private', '{{ ansible_env.HOME }}/Downloads', '{{ ansible_env.HOME }}/pbuilder', '{{ ansible_env.HOME }}/snap/chromium/common/.cache', '{{ ansible_env.HOME }}/.ansible/tmp', '{{ ansible_env.HOME }}/.config/Code/CachedData', '{{ ansible_env.HOME }}/.dropbox', '{{ ansible_env.HOME }}/.dropbox-dist', '{{ ansible_env.HOME }}/.local/share/Zeal/Zeal/docsets', '{{ ansible_env.HOME }}/.local/share/zeitgeist', '{{ ansible_env.HOME }}/.mozilla/firefox/default/storage', '{{ ansible_env.HOME }}/.vscode/extensions']

[org/gnome/deja-dup/remote]
uri='sftp://backup/srv/backup/{{ ansible_hostname }}'
folder=''
